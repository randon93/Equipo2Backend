<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Prestamo.
    -->
    <changeSet id="20201128194947-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="usuario_cliente_id"
                                 baseTableName="prestamo"
                                 constraintName="fk_prestamo_usuario_cliente_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuarios"/>

        <addForeignKeyConstraint baseColumnNames="usuario_biblioteca_id"
                                 baseTableName="prestamo"
                                 constraintName="fk_prestamo_usuario_biblioteca_id"
                                 referencedColumnNames="id"
                                 referencedTableName="usuarios"/>

        <addForeignKeyConstraint baseColumnNames="libro_id"
                                 baseTableName="prestamo"
                                 constraintName="fk_prestamo_libro_id"
                                 referencedColumnNames="id"
                                 referencedTableName="libro"/>

    </changeSet>
</databaseChangeLog>
